# Text2Loc Visionary 前端本机演示环境配置
# 用于本机直接访问演示

# API服务器地址（本机访问）
VITE_API_BASE_URL=http://localhost:8080

# WebSocket服务器地址
VITE_WS_BASE_URL=ws://localhost:8080

# API请求超时时间（毫秒）
VITE_API_TIMEOUT=60000

# 最大并发请求数
VITE_MAX_CONCURRENT_REQUESTS=10

# API密钥（可选）
VITE_API_KEY=

# 客户端版本
VITE_APP_VERSION=1.0.0-demo

# 缓存TTL（秒）
VITE_CACHE_TTL=300

# 是否启用API模拟模式
VITE_API_MOCK_MODE=false

# 是否启用详细日志
VITE_ENABLE_VERBOSE_LOGGING=true

# 功能开关
VITE_ENABLE_ENHANCED_MODE=true
VITE_ENABLE_3D_VISUALIZATION=true
VITE_ENABLE_VOICE_INPUT=false
VITE_ENABLE_OFFLINE_MODE=false
VITE_ENABLE_REALTIME_COLLAB=false
VITE_ENABLE_GPU_ACCELERATION=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ACCESSIBILITY_MODE=false

# 性能配置
VITE_3D_QUALITY=medium
VITE_POINTCLOUD_LOD=5

# 演示模式
VITE_DEMO_MODE=true
VITE_DEMO_TIMEOUT=300